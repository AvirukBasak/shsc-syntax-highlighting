{
  "name": "shsc-syntax-highlighting",
  "displayName": "Shsc Syntax Highlighting",
  "description": "Syntax highlighting for Shsc on Visual Studio Code",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "shsc",
      "aliases": ["Shsc", "shsc"],
      "extensions": [".shsc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "shsc",
      "scopeName": "source.shsc",
      "path": "./syntaxes/shsc.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AvirukBasak/shsc-syntax-highlighting.git"
  },
  "license": "MIT"
}
